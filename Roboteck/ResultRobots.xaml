<Window x:Class="Roboteck.ResultRobots"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Roboteck"
        mc:Ignorable="d"
        WindowState="Maximized"
        ResizeMode="NoResize"
        Title="Выставка работ" MinHeight="900" MinWidth="1600">
    <Grid Name="Result">
        <Grid.Background>
            <ImageBrush ImageSource="Properties/NameBack.png"/>
        </Grid.Background>
        <ScrollViewer Name="scroller" Margin="10" VerticalScrollBarVisibility="Hidden">
            <StackPanel Name="library"/>
        </ScrollViewer>

    </Grid>
</Window>
